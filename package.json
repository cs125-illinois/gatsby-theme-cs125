{
  "name": "@cs125/gatsby-theme-cs125",
  "description": "Gatsby theme and React components for CS 125.",
  "license": "MIT",
  "version": "2020.5.10",
  "main": "index.js",
  "scripts": {
    "backend": "npm run backend:up || npm run backend:down",
    "backend:down": "docker-compose --env-file .env.development -p $npm_package_name down",
    "backend:up": "docker-compose --env-file .env.development -p $npm_package_name up",
    "build:example": "gatsby build",
    "check": "npm run depcheck && npm run eslint && npm run tsc && npm run prettier",
    "clean": "gatsby clean",
    "cleanup": "mv gatsby-config.js.bac gatsby-config.js && git clean -df",
    "depcheck": "depcheck --ignore-dirs .cache,public,dist --ignores=ts-node,gatsby-plugin-*,@typescript-eslint/*,eslint-*,gatsby-source-*,gatsby-transformer-*,@mdx-js/mdx,node-sass,nodemon,types",
    "deploy": "npm run build gh-pages -d public",
    "develop": "gatsby develop -p 1234",
    "eslint": "eslint --fix src/ --max-warnings=0 --ext .js,.ts,.tsx",
    "ncu": "ncu",
    "postpublish": "mv gatsby-config.js.bac gatsby-config.js && git clean -df",
    "prepublishOnly": "npm run check && mv gatsby-config.js gatsby-config.js.bac && tsc",
    "serve": "gatsby serve -p 1234 -H 0.0.0.0",
    "start": "concurrently -n example,backend --handle-input \"nodemon\" \"npm run backend\"",
    "tsc": "tsc --noEmit",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,yaml}\" && prettier-package-json --write"
  },
  "dependencies": {
    "@cs125/element-tracker": "^2020.5.21",
    "@cs125/react-google-login": "^2020.5.6",
    "@material-ui/core": "^4.10.0",
    "@mdx-js/mdx": "^1.6.5",
    "@mdx-js/react": "^1.6.5",
    "@reach/router": "^1.3.3",
    "@types/mdx-js__react": "^1.5.2",
    "@types/react": "^16.9.35",
    "gatsby": "^2.22.15",
    "gatsby-image": "^2.4.5",
    "gatsby-plugin-env-variables": "^1.0.2",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-manifest": "^2.4.9",
    "gatsby-plugin-material-ui": "^2.1.9",
    "gatsby-plugin-mdx": "^1.2.13",
    "gatsby-plugin-react-helmet": "^3.3.2",
    "gatsby-plugin-sass": "^2.3.2",
    "gatsby-plugin-sharp": "^2.6.9",
    "gatsby-plugin-typescript": "^2.4.3",
    "gatsby-source-filesystem": "^2.3.8",
    "gatsby-transformer-sharp": "^2.5.3",
    "lorem-ipsum": "^2.0.3",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-ace": "^9.0.0",
    "react-children-utilities": "^2.1.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-icons": "^3.10.0",
    "runtypes": "^4.2.0",
    "slugify": "^1.4.0",
    "source-map-support": "^0.5.19",
    "throttle-debounce": "^2.1.0",
    "ts-node": "^8.10.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0"
  },
  "devDependencies": {
    "@types/react-helmet": "^6.0.0",
    "@types/throttle-debounce": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.2.0",
    "depcheck": "^0.9.2",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "gh-pages": "^3.0.0",
    "nodemon": "^2.0.4",
    "npm-check-updates": "^6.0.1",
    "prettier": "2.0.5",
    "prettier-package-json": "^2.1.3",
    "typescript": "^3.9.3"
  },
  "nodemonConfig": {
    "watch": [
      "package.json",
      "package-lock.json",
      "gatsby-config.js",
      "gatsby-config.ts",
      "gatsby-node.ts",
      "gatsby-browser.ts"
    ],
    "exec": "gatsby develop -p 1234 -H 0.0.0.0"
  },
  "prettier": {
    "semi": false,
    "arrowParens": "avoid",
    "trailingComma": "es5",
    "singleQuote": false,
    "printWidth": 120,
    "tabWidth": 2
  }
}
